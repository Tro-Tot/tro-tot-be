model Bill {
  id          String        @id @default(uuid()) @map("id")
  status      String        @map("status")
  dueDate     DateTime      @map("due_date")
  createdAt   DateTime      @default(now()) @map("created_at")
  updatedAt   DateTime      @updatedAt @map("updated_at")
  BillItem    BillItem[]
  Penalty     Penalty[]
  Transaction Transaction[]
}
