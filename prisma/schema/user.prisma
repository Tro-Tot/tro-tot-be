model User {
  id        String    @id @default(uuid())
  email     String    @unique
  name      String?
  roleId    String
  createdAt DateTime  @default(now())
  deletedAt DateTime?
  updatedAt DateTime  @updatedAt
  password  String
  username  String    @unique
  role      Role      @relation(fields: [roleId], references: [id])
}
