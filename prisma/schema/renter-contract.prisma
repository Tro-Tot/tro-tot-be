model RenterContract {
  id                        String                     @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  roomHistoryId             String                     @map("room_history_id") @db.Uuid
  depositBillId             String                     @unique @map("deposit_bill_id") @db.Uuid
  renterId                  String                     @map("renter_id") @db.Uuid
  totalMonths               Int                        @map("total_months")
  billingCycle              Int                        @map("billing_cycle")
  fromDate                  DateTime                   @map("from_date") @db.Timestamptz(6)
  toDate                    DateTime                   @map("to_date") @db.Timestamptz(6)
  signingDate               DateTime                   @map("signing_date") @db.Timestamptz(6)
  exterminateDate           DateTime                   @map("exterminate_date") @db.Timestamptz(6)
  status                    String                     @map("status")
  createdAt                 DateTime                   @default(now()) @map("created_at") @db.Timestamptz(6)
  updatedAt                 DateTime?                  @updatedAt @map("updated_at") @db.Timestamptz(6)
  deletedAt                 DateTime?                  @map("deleted_at") @db.Timestamptz(6)
  renterContractAttachments RenterContractAttachment[]
  depositBill               DepositBill                @relation(fields: [depositBillId], references: [id])
  renter                    Renter                     @relation(fields: [renterId], references: [id])
  roomHistory               RoomHistory                @relation(fields: [roomHistoryId], references: [id])

  @@map("renter_contracts")
}
