model ServiceUsage {
  id                        String                  @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  previousUsageNumber       Int?                    @map("previous_usage_number")
  currentUsageNumber        Int                     @map("current_usage_number")
  fromDate                  DateTime                @map("from_date") @db.Timestamptz(6)
  toDate                    DateTime                @default(now()) @map("to_date") @db.Timestamptz(6)
  isInitial                 Boolean                 @default(false) @map("is_initial")
  createdAt                 DateTime?               @default(now()) @map("created_at") @db.Timestamptz(6)
  updatedAt                 DateTime?               @updatedAt @map("updated_at") @db.Timestamptz(6)
  deletedAt                 DateTime?               @map("deleted_at") @db.Timestamptz(6)
  roomServiceSubscriptionId String                  @map("room_service_subscription_id") @db.Uuid
  billItems                 BillItem[]
  roomServiceSubscription   RoomServiceSubscription @relation(fields: [roomServiceSubscriptionId], references: [id])

  @@map("service_usages")
}
