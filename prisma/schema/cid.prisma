model Cid {
  number            String    @unique @map("number")
  issuingAuthority  String    @map("issuing_authority")
  issueDate         DateTime  @map("issue_date") @db.Timestamptz(6)
  registeredAddress String    @map("registered_address")
  createdAt         DateTime  @default(now()) @map("created_at") @db.Timestamptz(6)
  updatedAt         DateTime? @updatedAt @map("updated_at") @db.Timestamptz(6)
  deleted_at        DateTime? @map("deleted_at") @db.Timestamptz(6)
  id                String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  users             User[]

  @@map("cids")
}
