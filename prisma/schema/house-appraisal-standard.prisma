model HouseAppraisalStandard {
  @@map("house_appraisal_standard")
  id                String          @id @default(uuid()) @map("id")
  houseAppraisalId  String          @map("house_appraisal")
  standardId        String          @map("standard_id")
  createdAt         DateTime        @map("created_at")
  updatedAt         DateTime        @map("updated_at")
  deletedAt         DateTime?       @map("deleted_at")

  standard          Standard        @relation(fields: [standardId], references: [id])
  houseAppraisal    HouseAppraisal  @relation(fields: [houseAppraisalId], references: [id])
}
