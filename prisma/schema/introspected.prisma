model Cid {
  id                 String    @id
  number             String    @unique
  issuing_authority  String
  issue_date         DateTime
  registered_address String
  created_at         DateTime? @default(now())
  updated_at         DateTime?
  User               User[]
}

model LandLord {
  id        String   @id
  userId    String
  createdAt DateTime @default(now())
  updatedAt DateTime
  User      User     @relation(fields: [userId], references: [id])
}

model Manager {
  id        String   @id
  userId    String
  createdAt DateTime @default(now())
  updatedAt DateTime
  User      User     @relation(fields: [userId], references: [id])
}

model Renter {
  id        String   @id
  userId    String
  createdAt DateTime @default(now())
  updatedAt DateTime
  User      User     @relation(fields: [userId], references: [id])
}

model Staff {
  id        String   @id
  userId    String
  createdAt DateTime @default(now())
  updatedAt DateTime
  User      User     @relation(fields: [userId], references: [id])
}

model TechnicalStaff {
  id        String   @id
  userId    String
  createdAt DateTime @default(now())
  updatedAt DateTime
  User      User     @relation(fields: [userId], references: [id])
}

enum Gender {
  MALE
  FEMALE
  OTHER
}
