model Role {
  id        String    @id @default(uuid())
  name      String    @unique
  code      RoleCode
  createdAt DateTime  @default(now())
  deletedAt DateTime?
  updatedAt DateTime  @updatedAt
  users     User[]
}

enum RoleCode {
  RENTER
  LANDLORD
  BULDING_MANAGER
  COOPERATIVE_MANAGER
  ADMIN
}
