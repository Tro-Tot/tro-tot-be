model Role {
  id        String    @id @default(uuid())
  name      String    @unique
  code      RoleCode
  createdAt DateTime  @default(now())
  deletedAt DateTime?
  updatedAt DateTime  @updatedAt
  User      User[]
}

enum RoleCode {
  RENTER
  LANDLORD
  STAFF
  TECHNICAL_STAFF
  ADMIN
  MANAGER
}
